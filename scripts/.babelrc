{
  "presets": [
    "es2015", 
    "react", 
    "stage-2",
    ["env", {
      "targets": {
        "browsers": ["last 2 versions", "ie >= 10"]
      }
    }]
  ],
  "plugins": [
    "transform-class-properties",
    "add-module-exports",
    "transform-decorators-legacy",
    "ramda",
    "transform-export-extensions"
  ]
}
